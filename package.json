{
	"name": "pounce-ui",
	"version": "0.1.0",
	"description": "Custom elements built with pounce, pico.css, dockview and iconify",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev": "node --loader ts-node/esm ./node_modules/vite/bin/vite.js",
		"build": "tsc && node --loader ts-node/esm ./node_modules/vite/bin/vite.js build",
		"preview": "node --loader ts-node/esm ./node_modules/vite/bin/vite.js preview",
		"type-check": "tsc --noEmit",
		"lint": "biome check .",
		"test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
		"test:e2e": "playwright test"
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.28.4",
		"@babel/plugin-proposal-decorators": "^7.28.0",
		"@babel/plugin-transform-react-jsx": "^7.27.1",
		"@babel/plugin-transform-typescript": "^7.28.0",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-react": "^7.27.1",
		"@babel/preset-typescript": "^7.27.1",
		"@biomejs/biome": "^2.2.7",
		"@playwright/test": "^1.56.1",
		"@types/babel__core": "^7.20.5",
		"@types/jest": "^30.0.0",
		"@vitejs/plugin-legacy": "^7.2.1",
		"jest": "^30.2.0",
		"sass": "^1.93.2",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"vite": "^7.1.11",
		"vite-plugin-babel": "^1.3.2"
	},
	"dependencies": {
		"@iconify/iconify": "3.1.1",
		"@picocss/pico": "^2.0.6",
		"mutts": "file:../mutts",
		"pounce-ts": "file:../pounce-ts"
	}
}
